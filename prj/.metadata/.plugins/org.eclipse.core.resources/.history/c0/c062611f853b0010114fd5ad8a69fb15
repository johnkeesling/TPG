package edu.washington.ling575;

import com.google.appengine.api.datastore.Key;
import javax.jdo.annotations.IdGeneratorStrategy;
import javax.jdo.annotations.PersistenceCapable;
import javax.jdo.annotations.PrimaryKey;
import javax.jdo.annotations.Persistent;


/**
 * A very simple data model
 */
@PersistenceCapable
public class Expression {

	//A data class must have one field dedicated to storing the primary key of the corresponding datastore entity. 
	@PrimaryKey   
	@Persistent(valueStrategy = IdGeneratorStrategy.IDENTITY)
	private Key key;

	@Persistent
	private String form;

	@Persistent
	private String lang;

	/*
	 * constructor
	 */
	public Expression(String form, String lang){
		this.form = form;
		this.lang = lang;
	}
	

	//Accessors used by the app (not the JDO)

	public void setKey(Key key) {
        this.key = key;
    }
	
	public Key getKey() {
		return key;
	}

	public void setForm(String form){
		this.form = form;
	}
	
	public String getForm(){
		return this.form;
	}

	public void setLang(String lang){
		this.form = lang;
	}
	
	public String getLang(){
		return this.lang;
	}

	
	
}
